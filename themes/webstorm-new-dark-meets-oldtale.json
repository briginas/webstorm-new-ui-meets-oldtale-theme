  "semanticHighlighting": true,
  "semanticTokenColors": {
    "variable": { "foreground": "#aaa7b0" },
    "variable.defaultLibrary": { "foreground": "#c56584" },
    "type.defaultLibrary": { "fontStyle": "italic" },
    "parameter": { "foreground": "#aaa7b0" },
    "typeParameter": { "foreground": "#799f8a" },
    "type": { "foreground": "#cc9c86" },
    "interface": { "foreground": "#cc9c86" },
    "class": { "foreground": "#cc9c86" },
    "property": { "foreground": "#c56584" },
    "method": { "foreground": "#8292c7" },
    "function": { "foreground": "#8292c7" },
    "keyword": { "foreground": "#bf7c76" },
    "namespace": { "foreground": "#cc9c86" },
    "string": { "foreground": "#799f8a" },
    "number": { "foreground": "#9a8dbf" },
    "regexp": { "foreground": "#d68abc" },
    "operator": { "foreground": "#aaa7b0" }
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#7A7E85" }
    },
    {
      "name": "Ternary Operator",
      "scope": ["keyword.operator.ternary.ts", "keyword.operator.ternary.tsx"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression",
        "constant.language",
        "variable.language.this"
      ],
      "settings": { "foreground": "#bf7c76" }
    },
    {
      "name": "Logical Operator",
      "scope": ["keyword.operator.logical"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Optional Operator",
      "scope": ["keyword.operator.optional"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Definite Assignment Operator",
      "scope": [
        "keyword.operator.definiteassignment.ts",
        "keyword.operator.definiteassignment.tsx"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Relational Operator",
      "scope": [
        "keyword.operator.relational.ts",
        "keyword.operator.relational.tsx"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Assignment and Comparison Operators",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.comparison",
        "storage.type.function.arrow",
        "keyword.operator.union",
        "keyword.operator.bitwise"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Type Annotation Operator",
      "scope": [
        "keyword.operator.type.annotation.ts",
        "keyword.operator.type.annotation.tsx"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Type Operator",
      "scope": ["keyword.operator.type.ts", "keyword.operator.type.tsx"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Rest/Spread Operator",
      "scope": [
        "keyword.operator.rest.ts",
        "keyword.operator.rest.tsx",
        "keyword.operator.spread.ts",
        "keyword.operator.spread.tsx"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#799f8a" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#9a8dbf" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#8292c7" }
    },
    {
      "name": "Classes & React Components (Tags)",
      "scope": ["entity.name.class", "support.class", "entity.name.tag"],
      "settings": { "foreground": "#009385" }
    },
    {
      "name": "Types & Interfaces",
      "scope": ["entity.name.type", "support.type", "entity.name.interface"],
      "settings": { "foreground": "#cc9c86" }
    },
    {
      "name": "Primitive and Built-in Types",
      "scope": [
        "support.type.primitive.ts",
        "support.type.builtin.ts",
        "support.type.primitive.tsx",
        "support.type.builtin.tsx"
      ],
      "settings": { "foreground": "#bf7c76" }
    },
    {
      "name": "Enum name",
      "scope": ["entity.name.type.enum"],
      "settings": { "foreground": "#d59a82" }
    },
    {
      "name": "Enum member",
      "scope": ["variable.other.enummember"],
      "settings": { "foreground": "#f5938d" }
    },
    {
      "name": "Variables and Properties",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Exported Constants",
      // Note: This scope is not always effective as it can be
      // overridden by other scopes or semantic highlighting.
      "scope": ["meta.export variable.other.constant"],
      "settings": { "foreground": "#c56584" }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.type.property-name"
      ],
      "settings": { "foreground": "#c56584" }
    },
    {
      "name": "Tag attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": { "foreground": "#aaa7b0" }
    },
    {
      "name": "Invalid",
      "scope": ["invalid"],
      "settings": { "foreground": "#c56584" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold" }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": { "fontStyle": "strikethrough" }
    }
  ]
}
